# string
my_name = "Asahi"
print('my_name =', my_name)

# int
my_age = 20
print('esta es la edad =', my_age)

# boolean
is_single = False
print('is_single =', is_single)


# change variable
myname = "Alejandro"
print('myname =', myname)
print('my_name =', my_name)


# Inputs
my_name = input('¿Cuál es tu nombre?')
my_age = input('¿Cuántos años tienes?')
print(my_name)
print(my_age)
